{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/public-site/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/public-site/js/$1"
    },
    {
      "source": "/logo.png",
      "destination": "/public-site/logo.png"
    },
    {
      "source": "/logo.jpg",
      "destination": "/public-site/logo.jpg"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}